{
  "name": "bm-web",
  "version": "1.0.0",
  "description": "The Bruin Meet WebApp",
  "main": "server.js",
  "repository": "",
  "author": "Dmitri Brereton <mitrikyle@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --optimize-minimize",
    "build:production": "cross-env NODE_ENV=production yarn build",
    "build:staging": "cross-env NODE_ENV=staging yarn build",
    "build:stats": "cross-env NODE_ENV=production webpack --optimize-minimize --profile --json > .webpack-stats.json",
    "view-stats": "webpack-bundle-analyzer .webpack-stats.json public/",
    "analyze-build": "yarn build-with-stats && view-stats",
    "dev": "yarn && cross-env NODE_ENV=development webpack-dev-server",
    "flow": "flow",
    "format": "prettier --write \"js/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "watch": "webpack --watch",
    "precommit": "yarn run lint-staged",
    "test": "jest ./test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx}": ["yarn format", "git add"]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-register": "6.24.1",
    "chart.js": "^1.1.1",
    "clone": "^2.1.1",
    "color": "^2.0.1",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "dotty": "^0.1.0",
    "downshift": "^1.31.11",
    "express": "4.15.3",
    "immer": "^1.0.3",
    "moment": "^2.20.1",
    "polished": "^2.3.0",
    "prop-types": "15.5.10",
    "rc-slider": "^8.5.0",
    "react": "15.5.4",
    "react-addons-perf": "15.4.2",
    "react-chartjs": "^1.2.0",
    "react-countdown-now": "^1.2.0",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "4.1.1",
    "react-schedule-selector": "^1.0.3",
    "redux": "^3.7.2",
    "redux-thunk": "2.2.0",
    "styled-components": "2.0.0",
    "sweetalert": "^2.1.0",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-app": "^0.0.0",
    "babel-plugin-styled-components": "^1.6.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "1.1.7",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.0.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "lint-staged": "^7.1.2",
    "moxios": "0.4.0",
    "nodemon": "1.11.0",
    "prettier": "1.9.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.0.0",
    "style-loader": "^0.19.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "2.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120,
    "tabWidth": 2
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/test/__mocks__/fileMock.js"
    },
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "js"],
    "collectCoverageFrom": ["js/**"]
  }
}
